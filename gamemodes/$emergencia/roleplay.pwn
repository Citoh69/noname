}

funcion DestroyFire(fireid)
{
    if(Incendios[fireid][pIDI] != 0)
	{
	    if(Incendios[fireid][pType] == 1) i_Negocio[Incendios[fireid][pIDI]][EnLlamas] = 0;
        Incendios[fireid][pIDI] = 0;
        Incendios[fireid][pType] = 0;
        for(new i = 0; i < 15; i++)
		{
            if(Incendios[fireid][pObject][i])
