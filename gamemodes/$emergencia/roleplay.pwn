funcion _Expulsar(jugador, tipo, mensaje[]) {
    new InfoMjs[150];
    switch (tipo) {
        case 0: // Dialogo
            {
                format(InfoMjs, sizeof(InfoMjs), "{ffffff}Usted ha sido expulsado del servidor\n\n{771BA9}Razón:{ffffff}\n %s.", mensaje);
            	Dialog(jugador, D_INVALIDO, DIALOG_STYLE_MSGBOX, "{771BA9}[VC-RP]", InfoMjs, "Aceptar", "");
            	Expulsar(jugador, 1);
            }
        case 1: // Chat
            {
				format(InfoMjs, sizeof(InfoMjs), "[VC-RP]{ffffff}: Usted ha sido expulsado del servidor, razón: %s.", mensaje);
				SendClientMessage(jugador, 0x771BA9FF, InfoMjs);
            	Expulsar(jugador, 1);
			}
    }
    return 1;
}

