    if (LSPD_Veh(vehiculo) || SASD_Veh(vehiculo) || SAN_Vehicle(vehiculo) || GOB_Vehicle(vehiculo) || LSED_Vehicle(vehiculo)) { return _Mensaje(playerid, 0, "357", "No puedes abrir guanteras de facciones."); }
