        InfoVehFam[f][fv][vID] = Coche;
        SetVehicleHealth(Coche, health);
        SetVehicleVirtualWorld(Coche, vw);
        LinkVehicleToInterior(Coche, int);
        ActualizarFamilia(f);
        SetVehicleMatricula(Coche);
        return 1;
    }
    return 0;
}

