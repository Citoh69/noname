funcion A_Reloj()
{
	new
		RconCommand[50],
		time[3],
		year,
		month,
		day
	;
	getdate(year, month, day);
	gettime(time[0], time[1], time[2]);
	
	if(time[0] >= 12) format(RconCommand, 32, "worldtime %02d:%02dpm", (time[0] == 12) ? (12) : (time[0] - 12), time[1]);
	else if(time[0] < 12) format(RconCommand, 32, "worldtime %02d:%02dam", (time[0] == 0) ? (12) : (time[0]), time[1]);
	SendRconCommand(RconCommand);

	for (new playerid; playerid < MAX_PLAYERS; playerid++)
	{
		if (IsPlayerConnected(playerid))
		{
			SetPlayerTime(playerid, time[0], time[1]);
		}
	}
	
	return 1;
}

