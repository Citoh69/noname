							{
								if (IsPlayerInVehicle(p, tmpcar))
								{
									SetPlayerInterior(p, 0);
									SetPlayerVirtualWorld(p, 0);
									user[p][jVirtualWorld] = 0;
									user[p][jInterior] = 0;
								}
							}
							return 1;
						}
					}
				}
			}
		}
		else if (i_Empresa[i][eTipo] == 3)
  		{
		//
