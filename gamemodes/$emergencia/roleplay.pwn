stock DetectarCerrado(playerid, exception = INVALID_VEHICLE_ID) {
    new Float: Distance,
    target = -1;
    for(new v; v < MAX_VEHICLES; v++) if(VehNoExiste(v)) {
        if(v != exception && (target < 0 || Distance > CercaDeVehiculo(playerid, v))) {
            target = v;
            Distance = CercaDeVehiculo(playerid, v);
        }
    }
    return target;
}

