	if(ArmarioObj == 0) return _Mensaje(playerid, 0, "1", "Ésta ranura se encuentra vacía.");
 	if(ArmarioObj == 131)
	{
		if(user[playerid][jm_Derecha] == 0 && user[playerid][jm_Izquierda] == 0)
