funcion Loteria(number) {
    new DineroLoteriaFallen = 0, string[128], winner[50 + MAX_PLAYER_NAME];
    for(new i = 0; i < MAX_PLAYERS; i++) {
        if(IsPlayerConnected(i)) {
            if(user[i][jLoteria] > 0) {
                if(user[i][jLoteria] == number) {
                    DineroLoteriaFallen = 1;
                    GetPlayerName(i, winner, sizeof(winner));
                    format(string, sizeof(string), "[SAN] Ganador número '%d - %s', con un total de $%d", number, winner, DineroLoteria);
                    _MensajeSAN(0x00DC76FF, string);
                    format(string, sizeof(string), "[SMS: 9891]: Felicitaciones, has ganado $%d con tu boleta de loteria n°%d.", DineroLoteria, number);
                    _Mensaje(i, 4, "FFEA5F", string);
                    DarDineroGC(i, DineroLoteria);
                } else {
                    _Mensaje(i, 4, "cccccc", "No has ganado la loteria, suerte en la proxima.");
                }
            }
            user[i][jLoteria] = 0;
        }
    }
    if(DineroLoteriaFallen) {
        new rand = random(200);
        rand += 50;
        DineroLoteria = rand;
        ActualizarEst();
        format(string, sizeof(string), "[SAN] Número ganador %d, nuevo monto total de premio es de {00c200}$%d", number, DineroLoteria);
        _MensajeSAN(0x00DC76FF, string);
    } else {
        new rand = random(200);
        rand += 175;
        DineroLoteria += rand;
        ActualizarEst();
        format(string, sizeof(string), "[SAN] Número ganador %d, nuevo monto total de premio es de {00c200}$%d", number, DineroLoteria);
        _MensajeSAN(0x00DC76FF, string);
    }
    return 1;
}

