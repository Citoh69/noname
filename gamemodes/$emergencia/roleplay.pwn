			format(info, sizeof(info), "STAFF: %s (ID: %d - %s) | Rango: #%d - En servicio: %s", username[i], i, N_Apellido(i), user[i][jAdmin], EnServicioADM[i] == 1? ("Si") : ("No"));
			SendClientMessage(playerid, EnServicioADM[i] == 1? 0x7F0000FF : 0xEAFF68FF, info);
			check++;
		}
	}
	if(check == 0) return SendClientMessage(playerid, 0xB5B5B5FF, "No se encuentra ningún miembro STAFF en línea.");
	return 1;
}

