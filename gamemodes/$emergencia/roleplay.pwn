        format(InfoFamilia[fid][fRango2], 32, "");
        format(InfoFamilia[fid][fRango3], 32, "");
        format(InfoFamilia[fid][fRango4], 32, "");
        format(InfoFamilia[fid][fRango5], 32, "");
