					SetPlayerPos(playerid, i_Empresa[i][eExteriorX], i_Empresa[i][eExteriorY], i_Empresa[i][eExteriorZ]);
					SetPlayerFacingAngle(playerid, i_Empresa[i][eExteriorR]);
