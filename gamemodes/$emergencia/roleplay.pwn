				case 15: _Sangre(damagedid, -10.0); //- Palanca
				case 22: { //- 45. Colt
						switch(bodypart) {
							case 9: vida = 30;
			                case 3,4: vida = 12.0;
							case 5,6,7,8: vida = 8.0;
						}
					}
