funcion CargarGraffitis() {
    new Totalgffs, gfd[64];
    for (new gfid = 0; gfid < MAX_GRAFFITIS; gfid++)
	{
	    ResetGraffiti(gfid);
	    format(gfd, 64, DATOS_GRAFFITIS, gfid);
