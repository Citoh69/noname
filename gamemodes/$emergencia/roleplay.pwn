		for (new nid = 0; nid < MAX_NEGOCIOS; nid++) //negocios
		{
			if (_enPos(playerid, 2, i_Negocio[nid][nExteriorX], i_Negocio[nid][nExteriorY], i_Negocio[nid][nExteriorZ]))
			{
            	Intentar2[playerid] = gettime();
