funcion ActualizarHambre(playerid)
{
	if (user[playerid][jHambre] < MIN_HAMBRE) user[playerid][jHambre] = MIN_HAMBRE;
	if (user[playerid][jHambre] == MIN_HAMBRE * 2) _Mensaje(playerid, 4, "00A2D6", "Empiezas a tener mucha hambre. Deberías comer algo.");
