		ActualizarFamilia(user[playerid][jMiembroFam]);
		return 1;
	}
	else if(user[playerid][jm_Izquierda] == 0)
	{
		user[playerid][jm_Izquierda] = ArmarioObj;
		user[playerid][jm_IzquierdaCant] = Cant;
