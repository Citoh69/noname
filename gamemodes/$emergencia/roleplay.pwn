    	_Mensaje(playerid, 3, "0", "/tipoevento [tipo]");
    	_Mensaje(playerid, 2, "0", "Nombres: normal, cocheb, ninguno.");
    	return 1;
	}
	if (strcmp(params, "normal", true) == 0) {
  		InfoEvento[iTipo] = 1;
