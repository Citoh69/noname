    new str[124 + MAX_PLAYER_NAME + (8 * 3)];
    Loop(i, MAX_FAMILIAS, 1) {
        if (user[playerid][jLiderFam] == i) {
            format(str, sizeof(str), "[OOC FAMILIA] Líder %s: %s", NombreRJugador(playerid), params[0]);
            MensajeFamilia(i, 0x02C0F9FF, str);
